    "description": "FP Raycaster is a set of scriptable objects that help define and give the user a lot of control over building different Unity Raycasts. Think of this as a Unity Engine wrapper for raycasting. Currently supports 2D and 3D raycasts, Boxcasts, cubecasts, circlecasting, and sphere casting.",
    "unity": "6000.0",
    "unityRelease": "1f8",
    "documentationUrl": "https://github.com/jshull/FP_Ray/README.md",
    "changelogUrl": "https://github.com/jshull/FP_Ray/CHANGELOG.md",
    "licensesUrl": "https://github.com/jshull/FP_Ray/LICENSE.md",
    "repository": {
      "type": "git",
      "url": "https://github.com/jshull/FP_Ray.git"
    },
    "dependencies": {
        "com.unity.mathematics": "1.3.2",
        "com.unity.inputsystem": "1.17.0"
    },
    "samples": [
        {
            "displayName": "Scriptable Object Samples",
            "description": "Includes some examples of the different scriptable object based raycasts",
            "path": "Samples~/SamplesURP"
        }
    ],
    "keywords": [
      "ScriptableObject",
      "Raycast",
      "Physics2D",
      "Physics3D",
      "Spherecast",
      "Boxcast"
    ],
